{
  "name": "@porsche-design-system/components-angular",
  "version": "3.13.0",
  "description": "Porsche Design System is a component library designed to help developers create the best experience for software or services distributed by Dr. Ing. h.c. F. Porsche AG.",
  "keywords": [
    "porsche",
    "design",
    "system",
    "web",
    "component",
    "library",
    "angular",
    "js"
  ],
  "scope": "@porsche-design-system",
  "author": "Dr. Ing. h.c. F. Porsche AG",
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://designsystem.porsche.com",
  "dependencies": {
    "@porsche-design-system/components-js": "3.13.0"
  },
  "peerDependencies": {
    "@angular/common": ">=15.0.0 <18.0.0",
    "@angular/core": ">=15.0.0 <18.0.0",
    "rxjs": "^6.5.3 || ^7.4.0"
  },
  "exports": {
    "./jsdom-polyfill": {
      "types": "./jsdom-polyfill/index.d.ts",
      "default": "./jsdom-polyfill/index.cjs"
    },
    "./partials": {
      "types": "./partials/index.d.ts",
      "module": "./partials/index.js",
      "default": "./partials/index.cjs"
    },
    "./styles": {
      "sass": "./styles/_index.scss",
      "types": "./styles/esm/index.d.ts",
      "import": "./styles/esm/index.mjs",
      "default": "./styles/cjs/index.cjs"
    },
    "./testing": {
      "types": "./testing/index.d.ts",
      "default": "./testing/index.cjs"
    },
    "./package.json": "./package.json",
    ".": {
      "types": "./esm/index.d.ts",
      "esm2020": "./esm2020/porsche-design-system-components-angular.mjs",
      "es2020": "./fesm2020/porsche-design-system-components-angular.mjs",
      "es2015": "./fesm2015/porsche-design-system-components-angular.mjs",
      "node": "./fesm2015/porsche-design-system-components-angular.mjs",
      "default": "./fesm2020/porsche-design-system-components-angular.mjs"
    }
  },
  "module": "fesm2015/porsche-design-system-components-angular.mjs",
  "es2020": "fesm2020/porsche-design-system-components-angular.mjs",
  "esm2020": "esm2020/porsche-design-system-components-angular.mjs",
  "fesm2020": "fesm2020/porsche-design-system-components-angular.mjs",
  "fesm2015": "fesm2015/porsche-design-system-components-angular.mjs",
  "typings": "esm/index.d.ts",
  "sideEffects": false
}
